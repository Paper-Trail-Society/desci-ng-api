{
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.2.1",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "supabase": "^2.34.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "name": "desci-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "node --experimental-strip-types --experimental-transform-types --env-file=.env --watch src/index.ts",
    "dev:tsx": "npx tsx --env-file=.env --watch src/index.ts",
    "start": "node --env-file=.env dist/index.js",
    "netlify:dev": "netlify dev",
    "netlify:deploy": "netlify deploy --prod",
    "postinstall": "npm run build",
    "prepare:dev": "npm run build && npm run db:generate && npm run db:migrate && npm run db:seed",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node --env-file=.env dist/scripts/migrate.js",
    "db:check": "node --env-file=.env dist/scripts/check-schema.js",
    "db:seed": "node --env-file=.env dist/scripts/seed.js",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "academia",
    "project papers"
  ],
  "author": "Justin Irabor",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.55.0",
    "better-auth": "^1.3.6",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.44.4",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "netlify-cli": "^23.1.4",
    "pinata": "^2.4.9",
    "postgres": "^3.4.7",
    "serverless-http": "^3.2.0",
    "zeptomail": "^6.2.1",
    "zod": "^4.0.17"
  }
}
